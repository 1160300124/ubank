<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!--
	namespace：必须与对应的接口全类名一致
	id:必须与对应接口的某个对应的方法名一致

 -->
<mapper namespace="com.ulaiber.web.dao.LeaveAuditDao">

    <!--保存请假审批人-->
    <insert id="saveAditor" parameterType="java.util.List" >
        INSERT INTO tbl_leave_audit_record (recordNo,userid,auditor,auditDate,status)  VALUES
        <foreach collection="list" item="items" index= "index" separator =",">
            (#{items.recordNo},#{items.userid},#{items.auditor},#{items.auditDate},'0')
        </foreach>
    </insert>
</mapper>